{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/next"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ],
    "env": {
      "NODE_ENV": "production",
      "API_URL": "https://www.brownedgetechnology.com"
    }
  }
  