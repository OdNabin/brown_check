{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/next"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/hello"
      },
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ],
    "env": {
      "NODE_ENV": "production"
    }
  }
  